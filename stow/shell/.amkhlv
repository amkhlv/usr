# this file is supposed to be called in the non-interactive non-login shell
# for that, one needs to start the non-interactive non-login shell with the BASH_ENV=fullpathofthisfile

export PYTHONPATH=/home/andrei/usr/lib/python/

# set PATH so it includes user's private bin if it exists
export PATH="$PATH:$HOME/bin:$HOME/usr/bin:$HOME/usr/local/bin"
export MANPATH="$MANPATH:$HOME/usr/man:$HOME/usr/share/man:$HOME/usr/local/man:$HOME/usr/local/share/man"

export DBUS_SESSION_BUS_ADDRESS="unix:path=/var/run/user/1000/bus"

